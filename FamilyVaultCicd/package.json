{
  "name": "family_vault_cicd",
  "version": "0.1.0",
  "bin": {
    "family_vault_cicd": "bin/family_vault_cicd.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "install:all": "npm ci && npm ci --prefix lib/webhooktrigger/lambda",
    "lint": "eslint 'lib/**/*.ts'",
    "prettier": "prettier 'lib/**/*.ts'",
    "prettier-write": "prettier --write 'lib/**/*.ts'",
    "prettier-cicd": "node ./node_modules/prettier/bin/prettier.cjs 'lib/**/*.ts'",
    "lint-cicd": "node ./node_modules/eslint/bin/eslint.js 'lib/**/*.ts'",
    "deploy-cicd": "node ./node_modules/aws-cdk/bin/cdk deploy --require-approval never"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.16",
    "aws-cdk": "2.128.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.128.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
